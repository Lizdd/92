import random

# Функция для моделирования броска кубика
def roll_die():
    # Генерируем случайное число от 1 до 6, представляющее результат броска кубика
    return random.randint(1, 6)

# Функция для вывода результата броска кубика
def print_result(result):
    print(f"Выпало: {result}")

# Вызываем функцию броска кубика и выводим результат
print_result(roll_die())
